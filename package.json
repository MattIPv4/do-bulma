  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/MattIPv4/do-bulma/issues"
  },
  "homepage": "https://github.com/MattIPv4/do-bulma#readme",
  "dependencies": {
    "bulma": "^0.8.0"
  },
  "devDependencies": {
    "sass": "^1.23.0",
    "sass-lint": "^1.13.1"
  }
}
