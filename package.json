  "main": "src/style.scss",
  "scripts": {
    "test": "npm run lint && npm run build",
    "lint": "sass-lint src/{*,**/*}.scss --verbose --no-exit --config .sasslintrc",
    "build": "npm run build:global && npm run build:style",
